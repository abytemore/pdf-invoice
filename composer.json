{
    "name": "abytemore/pdf-invoice",
    "description": "Library to generate PDF invoices",
    "type": "library",
    "license": "GPL-2.0-or-later",
    "keywords": ["invoice", "pdf", "abytemore"],
    "support": {
        "source": "https://github.com/abytemore/pdf-invoice"
    },
    "authors": [
        {
            "name": "Ronald Mitterer",
            "homepage": "https://www.abytemore.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "setasign/fpdf": "~1.8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": { "abytemore\\PdfInvoice\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "abytemore\\PdfInvoice\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
